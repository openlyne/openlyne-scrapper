# Server
PORT=3000
NODE_ENV=development

# Auth (set either API_KEY or API_KEYS)
#API_KEY=changeme-single
#API_KEYS=key1,key2

# Scraping config
HEADLESS=true
SCRAPE_NAV_TIMEOUT_MS=45000
SCRAPE_MAX_CONCURRENCY=5
SCRAPE_DEFAULT_CONCURRENCY=3
ALLOW_MARKDOWN=true
SCREENSHOT_DIR=screenshots
REQUEST_BODY_LIMIT=1mb

# Rate limiting
RATE_LIMIT_WINDOW_MS=60000
RATE_LIMIT_MAX=60

# CORS (comma-separated origins, empty allows all)
#CORS_ALLOW_ORIGINS=https://yourapp.com,https://admin.yourapp.com

# Logging
LOG_LEVEL=info
